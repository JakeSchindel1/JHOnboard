{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/api/*"]
    },
    "platform": {
      "apiRuntime": "python:3.9"
    }
  }