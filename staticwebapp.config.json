{
  "routes": [
    {
      "route": "/api/intake",
      "methods": [
        "post"
      ],
      "rewrite": "/api/intake",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*",
      "/css/*",
      "/js/*"
    ]
  }
}