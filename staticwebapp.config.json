{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST"],
        "allowedRoles": ["anonymous"]
      },
      {
        "route": "/*",
        "rewrite": "/index.html",
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "responseOverrides": {
      "404": {
        "rewrite": "/index.html",
        "statusCode": 200
      }
    },
    "platform": {
      "apiRuntime": "node:18"
    },
    "framework": {
      "name": "next"
    },
    "mimeTypes": {
      ".json": "text/json",
      ".js": "text/javascript",
      ".css": "text/css",
      ".html": "text/html"
    }
  }