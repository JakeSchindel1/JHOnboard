{
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST"],
        "allowedRoles": ["anonymous"]
      }
    ],
    "navigationFallback": {
      "rewrite": "/_next/static/index.html",
      "exclude": [
        "/_next/static/*",
        "/_next/image",
        "/images/*",
        "/api/*"
      ]
    },
    "platform": {
      "apiRuntime": "node:18"
    },
    "framework": {
      "name": "nextjs"
    }
  }